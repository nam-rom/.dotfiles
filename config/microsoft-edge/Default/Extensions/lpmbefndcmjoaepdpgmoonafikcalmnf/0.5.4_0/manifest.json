{
   "background": {
      "persistent": false,
      "scripts": [ "config.js", "common.js" ]
   },
   "commands": {
      "toggle-reader-view": {
         "description": "Toggle the Reader View"
      }
   },
   "default_locale": "en",
   "description": "__MSG_description__",
   "homepage_url": "https://add0n.com/chrome-reader-view.html",
   "icons": {
      "128": "data/icons/128.png",
      "16": "data/icons/16.png",
      "256": "data/icons/256.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "512": "data/icons/512.png",
      "64": "data/icons/64.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 2,
   "name": "Reader View",
   "optional_permissions": [ "*://*/*", "webNavigation", "https://translate.google.com/" ],
   "options_ui": {
      "chrome_style": true,
      "open_in_tab": true,
      "page": "data/options/index.html"
   },
   "page_action": {
      "browser_style": true
   },
   "permissions": [ "storage", "activeTab", "contextMenus", "notifications", "declarativeContent", "chrome://favicon/", "https://text-to-speech-demo.ng.bluemix.net/api/*" ],
   "update_url": "https://edge.microsoft.com/extensionwebstorebase/v1/crx",
   "version": "0.5.4",
   "web_accessible_resources": [ "/data/reader/template.html" ]
}
